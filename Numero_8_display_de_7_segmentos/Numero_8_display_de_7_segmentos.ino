int e=1;
int d=2;
int c=4;

int a=7;
int f=9;
int b=6;
int g=10;
int punto=5;


void setup() {
  // put your setup code here, to run once:
  pinMode(e,OUTPUT);
  pinMode(d,OUTPUT);
  pinMode(c,OUTPUT);
  pinMode(a,OUTPUT);
  pinMode(f,OUTPUT);
  pinMode(b,OUTPUT);
  pinMode(g,OUTPUT);
  pinMode(punto,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(e,HIGH);
  digitalWrite(d,HIGH);
  digitalWrite(c,HIGH);
  digitalWrite(a,HIGH);
  digitalWrite(f,HIGH);
  digitalWrite(b,HIGH);
  digitalWrite(g,HIGH);
  digitalWrite(punto,HIGH);
}
